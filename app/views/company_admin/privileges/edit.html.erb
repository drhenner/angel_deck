

<%= render  :partial  => 'shared/breadcrumb_header',
            :locals   => { :left_header => "#{@employee.user.name.possessive} Privileges @ (#{company.name})"} %>

<%= form_for @employee, :url => company_admin_company_privilege_path(company, @employee) do |f| %>
  <%=  render :partial => 'form', :locals => {:f => f} %>
  <div class="actions"><%= f.submit 'Update' , :class => 'button' %></div>
<% end %>

